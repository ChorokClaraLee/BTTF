			alias="rejsdto" />
		<typeAlias type="kr.co.bttf.DTO.Reply_JSP_DTO"
			alias="rejspdto" />
		<typeAlias type="kr.co.bttf.DTO.Reply_ORACLE_DTO"
			alias="reoracledto" />
		<typeAlias type="kr.co.bttf.DTO.Reply_SPRING_DTO"
			alias="respringdto" />
		<typeAlias type="kr.co.bttf.DTO.AuthorityDTO"
			alias="authdto" />
		<typeAlias type="kr.co.bttf.DTO.HonoraryDTO"
			alias="honordto" />
		<typeAlias type="kr.co.bttf.DTO.UserDTO" alias="userdto" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver"
					value="oracle.jdbc.driver.OracleDriver" />
				<property name="url"
					value="jdbc:oracle:thin:@localhost:1521:xe" />
				<property name="username" value="bttf" />
				<property name="password" value="1234" />
			</dataSource>
		</environment>
	</environments>

